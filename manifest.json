{
  "name": "Interactive Markdown",
  "description": "Disrupt Bitches",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [{
    "js": [
      "serverRequest.js",
      "lib/vnc/webutil.js",
      "lib/vnc/base64.js",
      "lib/vnc/websock.js",
      "lib/vnc/keysymdef.js",
      "lib/vnc/keyboard.js",
      "lib/vnc/input.js",
      "lib/vnc/display.js",
      "lib/vnc/jsunzip.js",
      "lib/vnc/rfb.js",
      "lib/vnc/util.js",
      "lib/vnc/vnc.js",
      "inject.js",
      "grab.js",
      "lib/scriptTagContext.js",
      "lib/require.js",
      "lib/codeblock/js/bootstrap.js",
      "app.js"
    ],
    "css": [
      "lib/global.css",
      "lib/animate.css",
      "lib/codeblock/css/codeblock.css",
      "lib/codeblock/css/pygment_trac.css"
    ],
    "matches": [
      "*://*.com/*"
    ]
  }],
  "permissions": [
    "webRequest",
    "https://github.com/*",
    "http://github.com/*"
  ],
  "externally_connectable": {
    "matches": [
    ]
  },
  "web_accessible_resources": [
    "lib/require.js",
    "lib/jquery.js",
    "lib/codeblock/js/*",
    "lib/codeblock/vendor/*",
    "lib/codeblock/vendor/*",
    "lib/codeblock/css/*",
    "lib/codeblock/images/*",
    "lib/vnc/web-socket-js/*",
    "img/*",
    "popup.html"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "38": "icon38.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src 'self' data: chrome-extension-resource:;"
}